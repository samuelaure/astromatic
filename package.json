  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelaure/astromatic.git"
  },
  "homepage": "https://github.com/samuelaure/astromatic#readme",
  "bugs": {
    "url": "https://github.com/samuelaure/astromatic/issues"
  },
  "scripts": {
    "start": "tsx src/main.ts",
    "dev": "tsx src/dev.ts",
    "build": "tsc",
    "remotion": "remotion",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
